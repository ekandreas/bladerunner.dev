{
  "name": "ekandreas/bladerunner.dev",
  "type": "project",
  "license": "MIT",
  "description": "Dev env for Bladerunner (env based on roots/bedrock)",
  "authors": [
    {
      "name": "Andreas Ek",
      "email": "andreas@aekab.se"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "*",
    "wpackagist-plugin/oembed-gist":"*"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "require-dev": {
    "deployer/deployer":"*",
    "filp/whoops": "dev-master",
    "ekandreas/dipwpe": "*",
    "wpackagist-plugin/query-monitor": "^2.8"
  }
}
