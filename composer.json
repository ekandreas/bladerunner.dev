{
  "name": "ekandreas/bladerunner.dev",
  "type": "project",
  "license": "MIT",
  "description": "Dev env for Bladerunner (env based on roots/bedrock)",
  "authors": [
    {
      "name": "Andreas Ek",
      "email": "andreas@elseif.se"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "wpackagist-plugin/oembed-gist": "*",
    "johnpbloch/wordpress": "*",
    "wp-cli/wp-cli": "*"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "autoload": {
    "psr-4": {
      "Bladerunner\\": "web/app/plugins/bladerunner/src/"
    }
  },
  "require-dev": {
    "filp/whoops": "dev-master",
    "wpackagist-plugin/query-monitor": "*",
    "deployer/deployer": "~4"
  }
}
